{
  "description": "Block Google Analytics from any website you visit.",
  "homepage_url": "https://github.com/davidpob99/webextensions",
  "manifest_version": 2,
  "name": "Google Analytics Blocker",
  "permissions": [
    "management",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "icons": {
      "128": "icons/icon.svg"
  },
  "version": "1.1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon" : "icons/icon.svg"
  }
}
